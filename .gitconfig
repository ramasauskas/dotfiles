[core]
	pager = "diff-so-fancy | less --tabs=4 -RFX"
	editor = "nvim"
[interactive]
	diffFilter = "diff-so-fancy --patch"
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = "green bold"
	newHighlight = "green bold 22"
[color "diff"]
	meta = 11
	frag = magenta bold
	func = 146 bold
	commit = yellow bold
	old = red bold
	new = green bold
	whitespace = red reverse
[user]
	name = "Matas Rama≈°auskas"
	email = "public@ramasauskas.com"
[pull]
	rebase = true

[init]
	defaultBranch = master
